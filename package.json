{
  "name": "meli-challenge",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "apisauce": "^1.1.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "history": "^4.10.1",
    "i18next": "^13.1.5",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.32",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-spinkit": "^3.0.0",
